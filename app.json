{
  "expo": {
    "name": "LEORA",
    "slug": "leora-mobile",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "leora",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/iphone-splash-preview-dark.png",
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.leora.app",
      "splash": {
        "image": "./assets/images/iphone-splash-preview-dark.png",
        "resizeMode": "cover",
        "backgroundColor": "#000000"
      },
      "infoPlist": {
        "NSCameraUsageDescription": "LEORA needs access to your camera to capture receipts and documents.",
        "NSMicrophoneUsageDescription": "LEORA needs access to your microphone for voice commands.",
        "NSFaceIDUsageDescription": "LEORA uses Face ID to securely access your financial data.",
        "UIBackgroundModes": [
          "audio",
          "fetch",
          "remote-notification"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.leora.app",
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO",
        "USE_BIOMETRIC",
        "USE_FINGERPRINT",
        "VIBRATE",
        "RECEIVE_BOOT_COMPLETED",
        "SCHEDULE_EXACT_ALARM",
        "android.permission.USE_BIOMETRIC",
        "android.permission.USE_FINGERPRINT"
      ],
      "splash": {
        "image": "./assets/images/iphone-splash-preview-dark.png",
        "resizeMode": "cover",
        "backgroundColor": "#000000"
      }
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-local-authentication",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/notification-icon.png",
          "color": "#ffffff",
          "sounds": [
            "./assets/sounds/notification.mp3"
          ]
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "15.1"
          },
          "android": {
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "minSdkVersion": 24
          }
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "215c6b4e-64df-4adb-ae1b-ecf88f303e85"
      }
    },
    "owner": "dev_bek_211"
  }
}