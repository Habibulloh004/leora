{
  "expo": {
    "name": "LEORA",
    "slug": "leora",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "leora",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/iphone-splash-preview-dark.png",
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.leora.app",
      "splash": {
        "image": "./assets/images/iphone-splash-preview-dark.png",
        "resizeMode": "cover",
        "backgroundColor": "#000000"
      },
      "infoPlist": {
        "NSCameraUsageDescription": "LEORA needs access to your camera to capture receipts and documents.",
        "NSMicrophoneUsageDescription": "LEORA needs access to your microphone for voice commands.",
        "NSFaceIDUsageDescription": "LEORA uses Face ID to securely access your financial data.",
        "UIViewControllerBasedStatusBarAppearance": false,
        "UIBackgroundModes": [
          "audio",
          "fetch",
          "remote-notification",
          "audio",
          "fetch",
          "remote-notification"
        ],
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.leora.app",
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO",
        "USE_BIOMETRIC",
        "USE_FINGERPRINT",
        "VIBRATE",
        "RECEIVE_BOOT_COMPLETED",
        "SCHEDULE_EXACT_ALARM",
        "android.permission.USE_BIOMETRIC",
        "android.permission.USE_FINGERPRINT"
      ]
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/adaptive-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000000",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "react-native-vosk",
        {
          "models": [
            "assets/model-en-us"
          ]
        }
      ],
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Allow $(PRODUCT_NAME) to use Face ID."
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "5fa3a555-1519-4da3-9e1a-0879915b84d9"
      }
    },
    "owner": "habibulloh"
  }
}
